<h1>Create a Song</h1>


<%= form_for @song  do |f| %>	

  <%= f.text_field :title %>	  
  <%= f.text_field :artist_name %>

   <%= f.fields_for :notes do |note| %>
    <%= note.label :note %>
    <%= note.text_area :content, rows: 3, cols: 30 %>
  <% end %>
  <%= f.submit %>	  
<% end %>	  